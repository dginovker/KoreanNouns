{
	"manifest_version": 2,

	"name": "Korean Noun Replacer",
	"description": "Randomly replaces nouns with their Korean translated counterparts",
	"author": "dginovkerr",
	"version": "1.0",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
